{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: Runserver",
      "type": "debugpy",
      "request": "launch",
      "python": "${command:python.interpreterPath}",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "0.0.0.0:8000"
      ],
      "django": true,
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "DJANGO_SETTINGS_MODULE": "report_generator.settings",
        "PYTHONPATH": "${workspaceFolder}",
        "PYTHONUNBUFFERED": "1",
        "DEBUG": "True"
      },
      "console": "integratedTerminal"
    },

    {
      "name": "Django: Generate Stories id=x",
      "type": "debugpy",
      "request": "launch",
      "python": "${command:python.interpreterPath}",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "generate_stories",
        "--id",
        "71",
        "--force",
        "-v",
        "2"
      ],
      "django": true,
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "DJANGO_SETTINGS_MODULE": "report_generator.settings",
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal"
    },

